{
  "expo": {
    "name": "help-studies",
    "slug": "help-studies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "help-studies",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.helpstudies.app"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.helpstudies.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["@react-native-firebase/crashlytics"],
      "expo-router",
      "sentry-expo"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3a495f82-5b17-40dc-b102-ce5589cf46c4"
      }
    },
    "owner": "josecorte-exe"
  }
}
