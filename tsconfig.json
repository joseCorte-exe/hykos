{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@templates/*": [
        "src/templates/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@store/*": [
        "src/store/*"
      ]
    }
  }
}
